function calculateFactorial(number) {
  if (number < 0) return NaN;
  if (number === 0) return 1;
  let factorial = 1;
  for (let i = 1; i <= number; i++) {
    factorial *= i;
  }
  return factorial;
}

let number = 5;
let factorial = calculateFactorial(number);
console.log("Factorial of", number, "is:", factorial);
